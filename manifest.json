{
    "manifest_version": 3,
    "name": "Copy Portfolio",
    "version": "1.0",
    "description": "Copy Portfolio",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/logo.png"
      }
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]  
      }
    ]
}
